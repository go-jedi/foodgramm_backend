    swag init -g cmd/app/main.go

migrations:

create:
    migrate create -ext sql -dir migrations -seq create_users_table

execute:
    migrate -database postgresql://escort:escort@localhost:54321/escort_db?sslmode=disable -path migrations up
    migrate -database postgresql://escort:escort@localhost:54321/escort_db?sslmode=disable -path migrations down